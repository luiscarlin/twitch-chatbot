{
  "name": "spike",
  "packageManager": "yarn@3.1.1",
  "private": true,
  "workspaces": [
    "apps/*"
  ],
  "scripts": {
    "chatbot": "yarn workspace @spike/chatbot",
    "web": "yarn workspace @spike/web",
    "chatbot.start": "yarn chatbot start",
    "web.start": "yarn web start",
    "start": "yarn workspaces foreach -ipv run start"
  }
}
