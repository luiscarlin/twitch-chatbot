{
  "name": "spike",
  "packageManager": "yarn@3.1.1",
  "private": true,
  "workspaces": [
    "apps/*"
  ],
  "scripts": {
    "web": "yarn workspace @spike/web",
    "server": "yarn workspace @spike/server",
    "web.start": "yarn web start",
    "server.start": "yarn server start",
    "start": "yarn workspaces foreach -ipv run start"
  }
}
